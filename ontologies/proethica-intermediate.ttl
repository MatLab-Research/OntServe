proeth:MedicalEthicsCode a owl:Class ;
    rdfs:subClassOf proeth:ProfessionalCode ;
    rdfs:label "Medical Ethics Code"@en ;
    iao:0000115 "A formal code of ethics governing medical and biomedical practice."@en .

proeth:EngineeringEthicsCode a owl:Class ;
    rdfs:subClassOf proeth:ProfessionalCode ;
    rdfs:label "Engineering Ethics Code"@en ;
    iao:0000115 "A formal code of ethics governing engineering practice."@en .

proeth:TechnicalStandard a owl:Class ;
