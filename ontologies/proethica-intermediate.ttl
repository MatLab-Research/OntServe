# Professional Decision Validation
proeth:ProfessionalDecisionConstraint a sh:NodeShape ;
    sh:targetClass proeth:ProfessionalDecision ;
    sh:property [
        sh:path proeth:basedOnPrinciple ;
        sh:class proeth:Principle ;
        sh:minCount 1 ;
        sh:message "Every professional decision must be based on at least one ethical principle."@en ;
    ] ;
    sh:property [
        sh:path [ sh:inversePath proeth:linkedToProfessionalDecision ] ;
        sh:class proeth:EthicalAssessment ;
        sh:minCount 1 ;
        sh:message "Every professional decision should have at least one linked ethical assessment."@en ;
    ] .

# Formal Specification Compliance (D=R,P,O,S,Rs,A,E,Ca,Cs)
proeth:FormalSpecificationConstraint a sh:NodeShape ;
    sh:targetClass proeth:ProfessionalAction ;
    rdfs:comment "Ensures compliance with formal specification D=(R,P,O,S,Rs,A,E,Ca,Cs)"@en ;
    sh:property [
        sh:path [ sh:inversePath proeth:performsAction ] ;  # R - Role via Agent
        sh:class proeth:Agent ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path proeth:fulfillsObligation ;  # O - Obligation
        sh:class proeth:Obligation ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path proeth:hasState ;  # S - State
        sh:class proeth:State ;
        sh:minCount 0 ;  # Optional but recommended
    ] ;
    sh:property [
        sh:path proeth:usesResource ;  # Rs - Resource
        sh:class proeth:Resource ;
        sh:minCount 0 ;  # Optional
    ] ;
    sh:property [
        sh:path proeth:realizesCapability ;  # Ca - Capability
        sh:class proeth:Capability ;
        sh:minCount 1 ;
    ] .

